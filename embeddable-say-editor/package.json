{
  "name": "@lblod/embeddable-say-editor",
  "version": "5.7.0",
  "description": "Notule editor which may be embedded in another application.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lblod/frontend-embeddable-notule-editor"
  },
  "main": "dist/index.js",
