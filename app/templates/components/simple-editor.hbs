{{rdfa/rdfa-editor
  class="rdfa-editor"
  profile=profile
  value=model.content
  rdfaEditorInit=(action "handleRdfaEditorInit")
  editorOptions=(hash
    showToggleRdfaAnnotations="true"
    showInsertButton=null
    showRdfa="true"
    showRdfaHighlight="true"
    showRdfaHover="true"
  )
  toolbarOptions=(hash
    showTextStyleButtons="true"
    showListButtons="true" showIndentButtons="true"
  )
}}
{{yield}}
